{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "C++ Launch",
      "type": "cppdbg",
      "request": "launch",
      "program": "${fileDirname}\\${fileBasenameNoExtension}",
      "args": [
        "-g",
        "-std=c++11",
        "${file}",
        "-o",
        "${fileDirname}\\${fileBasenameNoExtension}"
      ],
      "environment": [
        {
          "name": "config",
          "value": "Debug"
        }
      ],
      "cwd": "${workspaceFolder}",
      
    },
    {
      "name": "C/C++ Runner: Debug Session",
      "type": "cppdbg",
      "request": "launch",
      "args": [],
      "stopAtEntry": false,
      "externalConsole": true,
      "cwd": "h:/GITHUB_WS/Problem_Solving/C++_Revision",
      "program": "h:/GITHUB_WS/Problem_Solving/C++_Revision/build/Debug/outDebug",
      "MIMode": "gdb",
      "miDebuggerPath": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}